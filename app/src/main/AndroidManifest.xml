<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pens.lab.app.belajaractivity">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Starter"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/TiaraTheme"
        android:usesCleartextTraffic="true">

        <activity android:name=".modul.add.AddActivity" />
        <activity android:name=".modul.edit.EditActivity" />
        <activity android:name=".modul.todo.ToDoActivity" />
        <activity android:name=".modul.signup.SignupActivity" />
        <activity android:name=".modul.signin.SigninActivity" />
        <activity android:name=".modul.main.MainActivity" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>